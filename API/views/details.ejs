  <link rel="stylesheet" href="/style.css">
  <title>Details</title>
</head>

<body>
  <%- include('includes/navbar', {isAuthenticated: isAuthenticated}); %>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">tenantId</th>
          <th scope="col">authContextId</th>
          <th scope="col">authContextDisplayName</th>
          <th scope="col">operation</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        <% acrsList.forEach(ac=> { %>
          <tr>
            <td>
              <%= ac.tenantId %>
                </th>
            <td>
              <%= ac.authContextId %>
            </td>
            <td>
              <%= ac.authContextDisplayName %>
            </td>
            <td>
              <%= ac.operation %>
            </td>
            <td>
              <form action='/admin/details?_method=DELETE' method='POST'>
                <input type='hidden' name='authContextId' value='<%= ac.authContextId %>'>
                <button type='submit'>Remove</button>
              </form>
            </td>
          </tr>
          <% }); %>
      </tbody>
    </table>

    <%- include('includes/footer'); %>
